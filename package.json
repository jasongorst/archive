    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "esbuild": "^0.23.1",
    "flowbite": "~2.5.1",
    "mrujs": "^1.0.2",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",

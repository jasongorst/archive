.search
  .page-header
    %h1 Search

  = render partial: 'form'

  - unless @results.nil?
    %hr
    .results
      - if @results.empty?
        %p No results found
      - else
        %small.text-muted #{@results.count} results
        = render partial: 'message', collection: @urls_results
        = paginate @results
